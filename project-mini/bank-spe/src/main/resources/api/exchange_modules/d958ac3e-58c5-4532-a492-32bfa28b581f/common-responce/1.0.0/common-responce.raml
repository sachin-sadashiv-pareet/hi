#%RAML 1.0 Trait


responses:
  400:
    body:
      application/json:
        example: {
          "meassage" : "bad request"
        }

  404 :
    body:
      application/json:
        example: {
          meassage : "resource not found"
        }
  500:
    body:
      application/json:
        example: {
          "meassage" : "server error"
        }

